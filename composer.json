{
    "name": "leaseweb/guzzle-bundle",
    "type": "symfony-bundle",
    "description": "Guzzle API call functionality with full Web Debug Toolbar integration",
    "keywords": ["API","Guzzle","cURL","Symfony2"],
    "homepage": "http://www.leaseweblabs.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Maurits van der Schee",
            "email": "m.vanderschee@leaseweb.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": ">=2.1",
        "guzzlehttp/guzzle": "*"
    },
    "autoload": {
        "psr-0": { "Lsw\\GuzzleBundle": "" }
    },
    "target-dir": "Lsw/GuzzleBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}

